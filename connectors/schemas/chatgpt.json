{
  "name": "ChatGPT",
  "version": "2.0.0",
  "scope": "chatgpt",
  "dialect": "json",
  "description": "All exported data from ChatGPT including conversation history and saved memories",
  "schema": {
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "description": "The email address associated with the ChatGPT account"
      },
      "memories": {
        "type": "array",
        "description": "Saved memories that ChatGPT uses to personalize responses",
        "items": {
          "type": "object",
          "properties": {
            "id": { "type": "string" },
            "content": { "type": "string" },
            "created_at": { "type": "string" },
            "updated_at": { "type": "string" },
            "type": { "type": "string" }
          },
          "required": ["id", "content", "created_at"]
        }
      },
      "conversations": {
        "type": "array",
        "description": "Full conversation history including all messages with ChatGPT",
        "items": {
          "type": "object",
          "properties": {
            "id": { "type": "string" },
            "title": { "type": "string" },
            "create_time": { "type": "string" },
            "update_time": { "type": "string" },
            "message_count": { "type": "number" },
            "messages": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": { "type": "string" },
                  "role": { "type": "string", "enum": ["user", "assistant"] },
                  "content": { "type": "string" },
                  "content_type": { "type": "string" },
                  "create_time": { "type": ["string", "null"] },
                  "model": { "type": ["string", "null"] }
                },
                "required": ["id", "role", "content"]
              }
            }
          },
          "required": ["id", "title", "create_time", "update_time", "messages"]
        }
      }
    },
    "required": ["email", "memories", "conversations"]
  }
}
