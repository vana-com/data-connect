{
  "name": "Instagram",
  "version": "2.0.0",
  "scope": "instagram",
  "dialect": "json",
  "description": "All exported data from Instagram including profile information and posts",
  "schema": {
    "type": "object",
    "properties": {
      "username": { "type": "string" },
      "full_name": { "type": "string" },
      "bio": { "type": "string" },
      "profile_pic_url": { "type": "string" },
      "external_url": { "type": ["string", "null"] },
      "follower_count": { "type": "number" },
      "following_count": { "type": "number" },
      "media_count": { "type": "number" },
      "is_private": { "type": "boolean" },
      "is_verified": { "type": "boolean" },
      "is_business": { "type": "boolean" },
      "posts": {
        "type": "array",
        "description": "Published posts from the user's feed",
        "items": {
          "type": "object",
          "properties": {
            "img_url": { "type": "string" },
            "caption": { "type": "string" },
            "num_of_likes": { "type": "number" },
            "who_liked": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "profile_pic_url": { "type": "string" },
                  "pk": { "type": "string" },
                  "username": { "type": "string" },
                  "id": { "type": "string" }
                }
              }
            }
          },
          "required": ["img_url", "caption", "num_of_likes"]
        }
      }
    },
    "required": ["username", "full_name", "posts"]
  }
}
