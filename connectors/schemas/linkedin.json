{
  "name": "LinkedIn",
  "version": "1.3.0",
  "scope": "linkedin",
  "dialect": "json",
  "description": "All exported data from LinkedIn including profile, work experience, education, skills, and languages",
  "schema": {
    "type": "object",
    "properties": {
      "profileUrl": {
        "type": "string",
        "description": "URL of the LinkedIn profile"
      },
      "fullName": {
        "type": "string",
        "description": "Full display name from the profile"
      },
      "headline": { "type": "string" },
      "location": { "type": "string" },
      "connections": {
        "type": "string",
        "description": "Connection count as displayed on profile (e.g. '500+')"
      },
      "profilePictureUrl": { "type": "string" },
      "about": { "type": "string" },
      "experience": {
        "type": "array",
        "description": "Work experience history",
        "items": {
          "type": "object",
          "properties": {
            "jobTitle": { "type": "string" },
            "companyName": { "type": "string" },
            "dates": { "type": "string" },
            "location": { "type": "string" },
            "description": { "type": "string" }
          },
          "required": ["jobTitle", "companyName"]
        }
      },
      "education": {
        "type": "array",
        "description": "Education history",
        "items": {
          "type": "object",
          "properties": {
            "schoolName": { "type": "string" },
            "degree": { "type": "string" },
            "years": { "type": "string" },
            "grade": { "type": "string" },
            "logoUrl": { "type": "string" }
          },
          "required": ["schoolName"]
        }
      },
      "skills": {
        "type": "array",
        "description": "Skills and endorsements",
        "items": {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "endorsements": { "type": "string" }
          },
          "required": ["name"]
        }
      },
      "languages": {
        "type": "array",
        "description": "Languages listed on profile",
        "items": {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "proficiency": { "type": "string" }
          },
          "required": ["name"]
        }
      }
    },
    "required": ["fullName"]
  }
}
